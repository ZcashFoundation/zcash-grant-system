    "node": "8.13.0"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/dev/index.ts",
    "genkey": "yarn run ts-node src/bin/genkey.ts",
