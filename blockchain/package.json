    "test": "yarn ts-mocha --no-colors src/**/*.spec.ts"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/expect": "^1.20.3",
    "@types/express": "4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.11",
    "isomorphic-ws": "^4.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "@types/cors": "2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/ws": "^6.0.1",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "dotenv": "^6.1.0",
    "express": "4.16.4",
    "redux": "4.0.1",
    "stdrpc": "1.0.0",
    "ws": "^6.1.2",
